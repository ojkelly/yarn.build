{
  "name": "@ojkelly/yarn-plugin-bundle",
  "version": "0.9.1",
  "main": "./src/index.ts",
  "homepage": "https://yarn.build",
  "license": "MIT",
  "devDependencies": {
    "@types/is-ci": "^2.0.0",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^4.0.2",
    "@types/yup": "^0.26.12",
    "@yarnpkg/builder": "*",
    "@yarnpkg/cli": "*",
    "@yarnpkg/core": "*",
    "@yarnpkg/fslib": "*",
    "@yarnpkg/libzip": "*",
    "@yarnpkg/plugin-pack": "*",
    "jest": "^26.6.3",
    "jest-pnp-resolver": "^1.2.1",
    "js-yaml": "^4.1.0",
    "replace": "^1.2.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.4.4",
    "tslib": "^2.3.0",
    "typanion": "^3.3.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@ojkelly/yarn-build-shared": "workspace:*",
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/auto-instrumentations-node": "^0.31.0",
    "@opentelemetry/resources": "^1.3.1",
    "@opentelemetry/sdk-node": "^0.29.2",
    "@opentelemetry/sdk-trace-base": "^1.3.1",
    "@opentelemetry/semantic-conventions": "^1.3.1",
    "@types/slice-ansi": "^4.0.0",
    "@yarnpkg/fslib": "*",
    "@yarnpkg/libzip": "*",
    "@yarnpkg/parsers": "*",
    "@yarnpkg/plugin-pack": "*",
    "@yarnpkg/plugin-pnp": "*",
    "@yarnpkg/pnp": "*",
    "@yarnpkg/pnpify": "*",
    "await-semaphore": "0.1.3",
    "clipanion": "^3.x",
    "date-fns": "^2.12.0",
    "esprima": "^4.0.1",
    "get-tsconfig": "^4.2.0",
    "globby": "^11.0.4",
    "ignore": "^5.1.8",
    "is-ci": "^2.0.0",
    "p-limit": "^2.3.0",
    "p-queue": "^6.3.0",
    "slice-ansi": "^4.0.0",
    "strip-ansi": "^6.0.0"
  },
  "scripts": {
    "dev": "yarn update:local",
    "build": "yarn build:plugin-stage",
    "build:plugin-stage": "builder build plugin",
    "update:workspace": "shx mkdir -p ../../../.yarn/plugins/@ojkelly && shx cp ./bundles/@yarnpkg/plugin-bundle.js ../../../.yarn/plugins/@ojkelly/plugin-bundle.cjs",
    "update:patch-name": "replace '@yarnpkg/plugin-bundle' '@ojkelly/plugin-bundle' ./bundles/@yarnpkg/plugin-bundle.js",
    "update": "yarn build:plugin-stage && yarn update:patch-name && yarn update:workspace",
    "update:local": "yarn build:plugin-stage && yarn update:patch-name && yarn update:workspace",
    "clean": "shx rm -fr build bundles",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/ojkelly/yarn.build.git"
  },
  "publishConfig": {
    "main": "./bundles/@ojkelly/plugin-bundle.js"
  },
  "files": [
    "/bundles/**/*"
  ],
  "engines": {
    "node": ">=12 <14 || 14.2 - 14.9 || >14.10.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "yarn.build": {
    "output": "bundles"
  }
}
